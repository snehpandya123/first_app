<!DOCTYPE html>
<html>
<head>
  <title>FirstApp</title>
  <%= stylesheet_link_tag :all %>
  <%= stylesheet_link_tag 'custom',:media => 'screen'%>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>
<body>
	
		<div class="container">
		<header>
			<%= image_tag("sneh.jpg", :alt => "Sample App", :class => "round") %>
				<nav class="round">
					<ul>
						<li><%= link_to "Home", '#' %></li>
						<li><%= link_to "Help", '#' %></li>
						<li><%= link_to "Sign in", '#' %></li>
					</ul>
				</nav>
		</header>
		<section class="round">		
			<div class="container" align="center">
			<%= yield %>
		</div>
		</section>
		

		<div align="center">
		<footer align="center">
			<nav class="round">
				<ul>
					<li><%= link_to "About", '#' %></li>
					<li><%= link_to "Contact", '#' %></li>
					<li><a href="http://news.railstutorial.org/">News</a></li>
					<li><a href="http://www.railstutorial.org/">Rails Tutorial</a></li>
				</ul>
			</nav>
		</footer>
	</div>

</body>



</body>
</html>
